{
  "name": "Set Aside Tabs",
  "minVersion": "59.0",
  "icons": {
    "48": "set-aside-icon-48.png",
    "96": "set-aside-icon-96.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "set-aside-icon-48.png",
    "default_title": "Set Aside Tabs"
  },
  "commands": {
    "set-aside-open-tabs": {
      "suggested_key": {
        "default": "Alt+Shift+U",
        "linux": "Ctrl+Shift+U"
      },
      "description": "Set aside all the open tabs"
    }
  },
  "permissions": [
    "<all_urls>",
    "sessions",
    "tabs",
    "storage",
    "management"
  ],
  "csp": [
    "script-src 'self'",
    "object-src 'self'",
    "img-src 'self' data:"
  ]
}