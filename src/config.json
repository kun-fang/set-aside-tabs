{
  "name": "Set Aside Tabs",
  "icons": {
    "48": "set-aside-icon-48.png",
    "96": "set-aside-icon-96.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "set-aside-icon-48.png",
    "default_title": "Set Aside Tabs"
  },
  "permissions": [
    "<all_urls>",
    "sessions",
    "tabs",
    "storage",
    "management"
  ],
  "csp": [
    "script-src 'self' 'unsafe-eval'",
    "object-src 'self'",
    "img-src 'self' data:"
  ]
}